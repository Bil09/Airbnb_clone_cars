<div class="container">
  <h1 class="mt-5 mb-3">Create new car</h1>
  <%= simple_form_for [@car] do |f| %>
    <%= f.input :brand %>
    <%= f.input :model %>
    <%= f.input :color %>
    <%= f.input :kilometers %>
    <%= f.input :year %>
    <%= f.input :fuel_consumption %>
    <%= f.input :price %>
    <%= f.input :hp %>
    <%= f.input :description %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>


    <%= f.submit "Submit review", class: "btn btn-primary my-5" %>
  <% end %>
</div>
