<div class="container">
  <h1>Bookings</h1>
  <% @bookings.each do |booking|  %>
    <div class="card-product my-3 rounded">
    <img src="https://cdn.motor1.com/images/mgl/n21yR/s3/ferrari-488-pista-im-test.jpg" />
    <div class="card-product-infos ">
    <h2>Enjoy the <%= booking.car.brand %> <%= booking.car.model %> in the color <%= booking.car.color %></h2>
    <%# <%= (booking.end_of_booking - booking.start_of_booking).to_i %> 
    </div>
  </div>
  <% end %>
</div>
